{
  "name": "webfly",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:use-cases": "cd use_cases && pnpm dev --host",
    "build": "tsc -b && vite build",
    "build:use-cases": "cd use_cases && pnpm install && pnpm build && cd .. && node scripts/copy-use-cases.js",
    "build:all": "pnpm build:use-cases && pnpm build",
    "lint": "eslint .",
    "preview": "vite preview",
    "flutter": "cd flutter && flutter run",
    "flutter:platforms": "rust-script flutter/tools/flutter-gen-platforms.rs --config flutter/app.pkl",
    "flutter:build-apk": "cd flutter && flutter build apk",
    "flutter:clean": "cd flutter && flutter clean"
  },
  "dependencies": {
    "@openwebf/react-router": "^0.24.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "npm:rolldown-vite@7.3.1",
    "vite-plugin-qrcode": "^0.3.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
